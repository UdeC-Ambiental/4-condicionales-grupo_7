   "source": [
    "#Clasificación de pH con los rangos\n",
    "def clasificar_ph(valor):\n",
    "    if valor < 6.5:\n",
    "        return \"Ácido\"\n",
    "    elif 6.5 <= valor <= 8.5:\n",
    "        return \"Neutro\"\n",
    "    else:\n",
    "        return \"Básico\"\n",
    "\n",
    "df[\"clasificacion_pH\"] = df[\"pH\"].apply(clasificar_ph)\n",
    "df\n"
   ]
  },
